{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "a6digital/laravel-default-profile-image": "^2.0",
        "alisalehi/laravel-lang-files-translator": "^1.0",
        "babymarkt/deepl-php-lib": "^1.0",
        "bacon/bacon-qr-code": "3.0",
        "barryvdh/laravel-dompdf": "^3.0",
        "beyondcode/laravel-websockets": "^1.14",
        "buglinjo/laravel-webp": "^2.3",
        "codezero/laravel-unique-translation": "^4.3",
        "convertio/convertio-php": "^0.4.0",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "*",
        "opcodesio/log-viewer": "^3.15",
        "phpreel/static": "^1.0",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-activitylog": "^4.9",
        "spatie/laravel-medialibrary": "^11.12",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-translatable": "^6.8",
        "tymon/jwt-auth": "*",
        "yajra/laravel-datatables": "^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }, "files":[
            "app/Http/Helper/helper.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
